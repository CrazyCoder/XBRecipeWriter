{
  "expo": {
    "name": "XBRecipeWriter",
    "slug": "XBRecipeWriter",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.terminaldisclaimer.XBRecipeWriter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.terminaldisclaimer.XBRecipeWriter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useLegacyPackaging": true,
            "kotlinVersion": "2.0.21"
          }
        }
      ],
      "expo-router",
      [
        "react-native-nfc-manager",
        {
          "includeNdefEntitlement": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/icon.png",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-share-intent",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "06ce7f2c-1928-4750-b8de-788849a53ad3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/06ce7f2c-1928-4750-b8de-788849a53ad3",
      "requestHeaders": {
        "expo-channel-name": "XBRecipeWriter_updates"
      }
    }
  }
}
